#include <stdio.h>

int main() {
    int n, i = 1;
    unsigned long long factorial = 1;
    printf("Enter an integer: ");
    scanf("%d", &n);

    // Calculating the factorial of n
    while (i <= n) {
        factorial *= i;
        i++;
    }

    printf("Factorial of %d = %llu\n", n, factorial);
    return 0;
}
